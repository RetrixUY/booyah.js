  "homepage": "https://github.com/retrixuy/booyapi/#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.0",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "babel-jest": "^27.0.6",
    "gts": "^3.1.0",
    "jest": "^27.0.3",
    "ts-jest": "27.0.2",
    "typedoc": "^0.21.2",
